        <file>
            <source>liquibase-2/liquibase-backend.properties</source>
            <filtered>true</filtered>
        </file>
        <file>
            <source>liquibase-2/liquibase-services.properties</source>
            <filtered>true</filtered>
        </file>
    </files>-->
    <dependencySets>
        <dependencySet>
            <outputDirectory>lib</outputDirectory>
        </dependencySet>
    </dependencySets>
</assembly>
